{
  "name": "dynamic-query-builder-client",
  "version": "1.0.0",
  "description": "Typescript client for Dynamic Query Builder",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "nodemon --watch src -e ts --exec 'npm run test'",
    "prepublish": "npm run test && npm run build",
    "pretest": "tsc",
    "test": "node ./dist/test/index.js",
    "test:debug": "node --inspect-brk ./dist/test/index.js",
    "coverage": "rimraf ./istanbul && tsc && istanbul cover dist/test/index.js --report html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oplog/dynamic-query-builder-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oplog/dynamic-query-builder-client/issues"
  },
  "homepage": "https://github.com/oplog/dynamic-query-builder-client#readme",
  "devDependencies": {
    "@types/node": "^10.11.3",
    "@types/tape": "^4.2.32",
    "istanbul": "^0.4.5",
    "ncp": "^2.0.0",
    "nodemon": "^1.18.2",
    "query-string-parser": "^0.2.1",
    "rimraf": "^2.6.2",
    "tape": "^4.9.1",
    "tsc": "^1.20150623.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "moment": "^2.22.2"
  }
}
